<script type='text/javascript'>
// <![CDATA[
    document.addEventListener("DOMContentLoaded", function() {
        const postsList = document.querySelector('.popular-posts-list');
        const posts = postsList.querySelectorAll('.grid-item');

        // Set the display style to flex for the list
        postsList.style.display = 'flex';
        postsList.style.flexWrap = 'wrap'; // Allow wrapping

        // Set each post to take up 50% of the width
        posts.forEach(post => {
            post.style.flex = '0 0 50%'; // 50% width for 2 columns
            post.style.boxSizing = 'border-box'; // Ensure padding and border are included in the width
        });
    });
// ]]>
</script>
    <script type='text/javascript'>
  <![CDATA[
    function toggleDropdown() {
      const dropdown = document.querySelector('.dropdown-menu');
      dropdown.style.display = dropdown.style.display === 'block' ? 'none' : 'block';
    }
  ]]>
</script>
